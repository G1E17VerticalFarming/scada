<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="scada.gui.SceneLoginController">
   <children>
      <Label layoutX="251.0" layoutY="14.0" text="Login" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="100.0" layoutY="100.0" text="Username" />
      <Label layoutX="100.0" layoutY="200.0" text="Password" />
      <TextField fx:id="usernameTextField" layoutX="250.0" layoutY="100.0" />
      <TextField fx:id="passwordTextField" layoutX="250.0" layoutY="200.0" />
      <Button fx:id="cancelButton" layoutX="200.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Cancel" />
      <Button fx:id="loginButton" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Log in" />
   </children>
</AnchorPane>
